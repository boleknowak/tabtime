{
  "name": "tabtime",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run typecheck && npm run format:fix && npm run lint:fix && prisma migrate deploy && npx prisma db push --skip-generate && next build && npm run sitemap:generate",
